// -----------------------------------------------------
// Core.Data.Functor

<- Functor(..)
   <$>

// -----------------------------------------------------
// Functor Typeclass
Functor f :-
    map ? (a -> b) -> f a -> f b
    
    (<$) 4L
      ? a -> f b -> f a
      = map . const
    
// -----------------------------------------------------
// Extra Operators 
(<$>) 4L
  ? Functor f => (a -> b) -> f a -> f b
  = map