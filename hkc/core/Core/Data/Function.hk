<- identity
   const
   .
   flip
   $
   &

identity x
  ? a -> a
  = x
  

const x _
  ? a -> b -> a
  = x
  
 
(_._) 9R g f x
  ? (b -> c) -> (a -> b) -> a -> c
  = g (f x)
 

flip f x y
  ? (a -> b -> c) -> b -> a -> c
  = f y x
  
(_$_) 0R f x
  ? (a -> b) -> a -> b
  = f x
  
  
(_&_) 1L x f
  ? a -> (a -> b) -> b
  = f x